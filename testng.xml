<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" parallel="classes" thread-count="2">
    <parameter name="browser" value="edge"></parameter>
    <test verbose="2" preserve-order="true" name="Registration">
        <classes>
            <class name="org.iti.tests.RegistrationTest"/>
        </classes>
    </test>

    <test verbose="2" preserve-order="true" name="Login">
        <classes>
            <class name="org.iti.tests.LoginTest"/>
        </classes>
    </test>
</suite>